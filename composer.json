{
    "name": "appsdept/laravel-directpay-ipg",
    "description": "A simple way to connect directpay with laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AppsDept\\LaravelDirectpayIpg\\": "src/"
        },
        "files": [
            "Helpers/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "LasanthaSudeera",
            "email": "lasan.sudeera@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                 "AppsDept\\LaravelDirectpayIpg\\DirectpayServiceProvider"
            ],
            "aliases": {
                "Directpay": "AppsDept\\LaravelDirectpayIpg\\Facades\\DirectPay"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "8.0.x-dev"
    }
}
